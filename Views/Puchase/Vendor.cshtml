
@{
    ViewData["Title"] = "Vendor";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- start: page -->
<section class="panel" id="app-container">
    <header class="panel-heading">
        <div class="panel-actions">
            <a href="#" class="fa fa-caret-down"></a>
            <a href="#" class="fa fa-times"></a>
        </div>
        <h2 class="panel-title">Default</h2>
    </header>
    <div class="panel-body">
        <div class="row">
            <div class="col-sm-6">
                <div class="mb-md">
                    <button id="addToTable" class="btn btn-primary" data-toggle="modal" data-target="#vendorModal">Add <i class="fa fa-plus"></i></button>
                </div>
            </div>
        </div>
        <table class="table table-bordered table-striped mb-none" id="datatable-editable">
            <thead>
                <tr>
                    <th>Vendor Name</th>
                    <th>Vendor Email</th>
                    <th>Mobile Number</th>
                    <th>Pan Card</th>
                    <th>ADHAAR Card</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <tr class="gradeX" v-for="(item, index) in vendors">
                    <td>{{item.name}}</td>
                    <td>
                        {{item.email}}
                    </td>
                    <td>{{item.mobileNo}}</td>
                    <td>{{item.panCard}}</td>
                    <td>{{item.adhaarCard}}</td>
                    <td class="actions">
                        <a href="#" class="hidden on-editing save-row"><i class="fa fa-save"></i></a>
                        <a href="#" class="hidden on-editing cancel-row"><i class="fa fa-times"></i></a>
                        <a href="#" class="on-default edit-row"><i class="fa fa-pencil"></i></a>
                        <a href="#" class="on-default remove-row"><i class="fa fa-trash-o"></i></a>
                    </td>
                </tr>
            </tbody>
        </table>
    </div><!-- Modal -->
    <div class="modal fade" id="vendorModal" tabindex="-1" role="dialog" aria-labelledby="vendorModalModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Vendor Entry</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-xs-12">
                            <section class="panel">
                                <header class="panel-heading">
                                    <div class="panel-actions">
                                        <a href="#" class="fa fa-caret-down"></a>
                                        <a href="#" class="fa fa-times"></a>
                                    </div>

                                    <h2 class="panel-title">Vendor Entry</h2>
                                </header>
                                <div class="panel-body">
                                    <form class="form-horizontal form-bordered">
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Vendor Name</label>
                                            <div class="col-md-6">
                                                    <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtVendorName" 
                                                       name="name" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)"
                                                       >
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorEmail">Vendor Email</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtVendorEmail" 
                                                       name="email" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Vendor Mobile Number</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtMobileNo" 
                                                       name="mobileNo" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Vendor Age</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtAge" 
                                                       name="age" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Residential Address</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtperAddress" 
                                                       name="perAddress" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Current Address</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtcurrentAddress" 
                                                       name="currentAddress" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Vendor Pan Card</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded"
                                                       id="txtPanCard" 
                                                       name="panCard" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Vendor Adhaar Card</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtAdhaarCard" 
                                                       name="adhaarCard" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">GST NUmber</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtMobileNo" 
                                                       name="gstNo" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Driving License</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtgstNo" 
                                                       name="dlNo" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Station</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtDlNo"
                                                       name="station" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Vehical No</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtStation" 
                                                       name="vehicalNo" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Medical Center</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtVehicalNo" 
                                                       name="medicalCenter" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">GSTN CN</label>
                                            <div class="col-md-6">
                                                <input type="text" 
                                                       class="form-control input-rounded" 
                                                       id="txtMedicalNumber" 
                                                       name="gstnCN" 
                                                       autocomplete="off"
                                                          v-on:input="handleChange($event)">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="txtVendorName">Company</label>
                                            <div class="col-md-6">
                                                <select class="form-control info" id="ddlCompany">
                                                    <option value="A">A</option>
                                                    <option value="B">B</option>
                                                    <option value="C">C</option>
                                                </select>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </section>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" v-on:click="onSubmit">Save changes</button>
                </div>
            </div>
        </div>
    </div>

</section>
<!-- end: page -->
@section scripts {
    <script src="~/Custom-Script/vendor.js"></script>
}